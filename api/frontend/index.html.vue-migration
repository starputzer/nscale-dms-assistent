    <!-- Port 8080 Fix - Hilft bei der Fehlerbehandlung -->
    <script>
      // Globales Fehlerhandling f√ºr ES-Module und Asset-Loading
      window.addEventListener('error', function(e) {
        console.error('[vue-error-handler]', e.message);
        
        // Nur bei Asset-Fehlern eingreifen
        if (e.target && (e.target.tagName === 'LINK' || e.target.tagName === 'SCRIPT')) {
          var src = e.target.src || e.target.href;
          if (!src) return;
          
          console.warn('[vue-error-handler] Asset-Ladefehler:', src);
          
          // Alternative Assets versuchen
          if (src.startsWith('/')) {
            // Absolute Pfade mit relativen ersetzen
            var newSrc = '.' + src;
            console.log('[vue-error-handler] Versuche relativen Pfad:', newSrc);
            
            if (e.target.tagName === 'LINK') {
              e.target.href = newSrc;
            } else {
              e.target.src = newSrc;
            }
          }
        }
      }, true);
    </script>
    
    <script type="module" crossorigin src="./assets/js/main.d053e982.js"></script>
