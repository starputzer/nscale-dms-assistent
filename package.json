{
  "name": "nscale-dms-assistent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:no-check": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production --analyze",
    "preview": "vite preview",
    "check": "vue-tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:vanilla": "vitest run -c vanilla.vitest.config.js",
    "test:vanilla:watch": "vitest -c vanilla.vitest.config.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report e2e-test-results/html",
    "deploy:dev": "bash scripts/deploy-dev.sh",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:prod": "bash scripts/deploy-production.sh",
    "rollback": "bash scripts/rollback.sh",
    "cache:invalidate": "bash scripts/cache-invalidate.sh",
    "build-and-serve": "bash scripts/build-and-serve.sh"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.0",
    "marked": "^9.1.6",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "terser": "^5.24.0",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.20"
  }
}